
<h1 class="mt-4"><%= user.name %></h1>
<div>
    <h6><i class="fas fa-coins"></i>  <%= user.gold %></h6>
    <h6><i class="far fa-gem"></i>  <%= user.gems %></h6>
    <h6>Lvl  <%= user.level %></h6>
    <h6>EXP  <%= user.experience %></h6>
    <h6><i class="fas fa-key"></i>  <%= user['gold keys'] %></h6>
</div>
<br>
<form action="/dashboard/spendGold" method="POST">
    <button class="btn btn-success" type="submit">Spend 1000 Gold</button>
</form>

